{
  "name": "remote-chess-engine",
  "version": "0.3.0",
  "description": "Remote Chess Engine, ready to process position analysis tasks via Remote Interface for Chess Positions Analysis (RICPA)",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "test": "jasmine",
    "tdd": "nodemon --exec jasmine",
    "start": "nodemon server.js",
    "coverage": "istanbul cover node_modules/jasmine/bin/jasmine.js",
    "e2e": "start node server.js && jasmine ./spec/e2e.js"
  },
  "keywords": [
    "chess",
    "fen",
    "analysis",
    "positions",
    "ricpa",
    "engine",
    "remote",
    "interface"
  ],
  "author": "Mikhail Subach",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.99.0",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Scorpibear/remote-chess-engine.git"
  },
  "dependencies": {
    "chess.js": "^0.10.2",
    "config": "^1.30.0",
    "express": "^4.16.2",
    "fen-analyzer": "^0.2.0",
    "uci-adapter": "github:Scorpibear/uci-adapter"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
